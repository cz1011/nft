<+>package com.shitouren.core.service;\r\n\r\nimport com.shitouren.core.autogenerate.bean.*;\r\nimport com.shitouren.core.autogenerate.dao.UsersDao;\r\nimport com.shitouren.core.autogenerate.dao.UserwinecellarDao;\r\nimport com.shitouren.core.autogenerate.dao.WinecellarDao;\r\nimport com.shitouren.core.config.exception.CloudException;\r\nimport com.shitouren.core.config.exception.ExceptionConstant;\r\nimport org.apache.catalina.User;\r\nimport org.springframework.beans.factory.annotation.Autowired;\r\nimport org.springframework.stereotype.Service;\r\n\r\nimport java.math.BigDecimal;\r\nimport java.util.*;\r\n\r\n@Service\r\npublic class WinecellarServiceImpl implements WinecellarService {\r\n    @Autowired(required = false)\r\n    UsersDao usersDao;\r\n\r\n    @Autowired(required = false)\r\n    WinecellarDao winecellarDao;\r\n\r\n    @Autowired(required = false)\r\n    UserwinecellarDao userwinecellarDao;\r\n\r\n    @Override\r\n    public Map winecellarList(int userid) {\r\n        Map mover = new HashMap();\r\n        List<Winecellar> winecellarList = winecellarDao.selectByExample(new WinecellarExample());\r\n        UserwinecellarExample userwinecellarExample = new UserwinecellarExample();\r\n        userwinecellarExample.createCriteria().andUseridEqualTo(userid);\r\n        List<Userwinecellar> userwinecellarList = userwinecellarDao.selectByExample(userwinecellarExample);\r\n        if (userwinecellarList.size() > 0) {\r\n            Userwinecellar userwinecellar = userwinecellarList.get(0);\r\n            mover.put(\"profit\", userwinecellar.getProfit());\r\n            mover.put(\"countdown\", userwinecellar.getCountdown());\r\n        } else {\r\n            mover.put(\"profit\", \"\");\r\n            mover.put(\"countdown\", \"\");\r\n        }\r\n        List list = new ArrayList();\r\n        for (Winecellar winecellar : winecellarList) {\r\n            Map map = new HashMap();\r\n            map.put(\"id\", winecellar.getId());\r\n            map.put(\"count\", winecellar.getCount());\r\n            map.put(\"cycle\", winecellar.getCycle());\r\n            list.add(map);\r\n        }\r\n        mover.put(\"winecellar\", list);\r\n        return mover;\r\n    }\r\n\r\n    @Override\r\n    public int createUserwinecellar(int userid, int id) {\r\n        UserwinecellarExample userwinecellarExample = new UserwinecellarExample();\r\n        userwinecellarExample.createCriteria().andUseridEqualTo(userid);\r\n        List<Userwinecellar> userwinecellarList = userwinecellarDao.selectByExample(userwinecellarExample);\r\n        if (userwinecellarList.size() > 0) {\r\n            throw new CloudException(ExceptionConstant.您已存入过酒窖);\r\n        }\r\n        Winecellar winecellar = winecellarDao.selectByPrimaryKey(id);\r\n        Users users = usersDao.selectByPrimaryKey(userid);\r\n        if (users.getWineDrop().compareTo(winecellar.getCount()) == -1) {\r\n            throw new CloudException(ExceptionConstant.您的酒滴不足);\r\n        }\r\n        //条件满足 存入 扣除用户酒滴\r\n        Users users1 = new Users();\r\n        users1.setUserId(userid);\r\n        users1.setWineDrop(users.getWineDrop().subtract(winecellar.getCount()));\r\n        usersDao.updateByPrimaryKeySelective(users1);\r\n        Userwinecellar userwinecellar = new Userwinecellar();\r\n        userwinecellar.setUserid(userid);\r\n        userwinecellar.setCreatetime(new Date());\r\n        userwinecellar.setProfit(winecellar.getCount());\r\n        userwinecellar.setCountdown(winecellar.getCycle());\r\n        return userwinecellarDao.insertSelective(userwinecellar);\r\n    }\r\n\r\n    @Override\r\n    public void winecellarOver() {\r\n        UserwinecellarExample userwinecellarExample = new UserwinecellarExample();\r\n        userwinecellarExample.createCriteria().andStateEqualTo(0);\r\n        List<Userwinecellar> userwinecellarList = userwinecellarDao.selectByExample(userwinecellarExample);\r\n        if (userwinecellarList.size() == 0) {\r\n            System.out.println(\"无酒窖\");\r\n        } else {\r\n            for (Userwinecellar userwinecellar : userwinecellarList) {\r\n                //将酒窖倒计时减少\r\n                Userwinecellar userwinecellar1 = new Userwinecellar();\r\n                int count = userwinecellar.getCountdown() - 1;\r\n                userwinecellar1.setId(userwinecellar.getId());\r\n                userwinecellar1.setCountdown(count);\r\n                //发酵完毕返还\r\n                if (count == 0) {\r\n                    Users user = usersDao.selectByPrimaryKey(userwinecellar.getUserid());\r\n                    Users users = new Users();\r\n                    users.setUserId(userwinecellar.getUserid());\r\n                    BigDecimal syjd = user.getWineDrop().add(userwinecellar.getProfit());\r\n                    users.setWineDrop(syjd);\r\n                    users.setIsFinishiCellar(1);\r\n                    usersDao.updateByPrimaryKeySelective(users);\r\n                    userwinecellar1.setState(1);//改为已酿完\r\n                }\r\n                userwinecellarDao.updateByPrimaryKeySelective(userwinecellar1);\r\n            }\r\n        }\r\n        System.out.println(\"已酿完\");\r\n    }\r\n\r\n}\r\n
