eger userId = userParam.getLogUserPid();\r\n        realNameService.addRealName(realNameParam, userId);\r\n    }\r\n\r\n    @PostMapping(\"/mine/getrealname\")\r\n    @ApiOperation(\"获取实名认证信息\")\r\n    @GetLoginUser\r\n    @Access\r\n    public RealNameVo getRealName(SysUserParam userParam) {\r\n        return realNameService.getRealName(userParam);\r\n    }\r\n\r\n    @PostMapping(\"/mine/massage\")\r\n    @ApiOperation(\"系统消息\")\r\n    @GetLoginUser\r\n    @Access\r\n    public List getMassageList() {\r\n        MassageExample massageExample = new MassageExample();\r\n        massageExample.createCriteria().andIdIsNotNull();\r\n        List<Massage> massageList = massageDao.selectByExample(massageExample);\r\n        List mapList = new ArrayList();\r\n        for (Massage massage : massageList) {\r\n            Map<String, Object> map = new HashMap<>();\r\n            map.put(\"title\", massage.getTitle());\r\n            map.put(\"contant\", massage.getContant());\r\n            map.put(\"date\", DateUtils.getDateToStr(massage.getDate(), \"yyyy-MM-dd\"));\r\n            mapList.add(map);\r\n        }\r\n        return mapList;\r\n    }\r\n\r\n    @PostMapping(\"/mine/mine\")\r\n    @ApiOperation(\"我的信息\")\r\n    @GetLoginUser\r\n    @Access\r\n    public Map<String, Object> mine(SysUserParam sysUserParam) {\r\n        Integer userId = sysUserParam.getLogUserPid();\r\n        return mineService.getMineInfo(userId);\r\n    }\r\n\r\n    @PostMapping(\"/mine/updateUserInfo\")\r\n    @ApiOperation(\"修改头像和昵称\")\r\n    @GetLoginUser\r\n    @Access\r\n    public void updateAvatar(SysUserParam sysUserParam, String avatar, String nickname) {\r\n        Integer userId = sysUserParam.getLogUserPid();\r\n        if (!StringUtils.isEmpty(avatar)) {\r\n            userService.updateAvatar(userId, avatar);\r\n        }\r\n        if (!StringUtils.isEmpty(nickname)) {\r\n            userService.updateNickname(userId, nickname);\r\n        }\r\n\r\n\r\n    }\r\n\r\n    @PostMapping(\"/mine/inviteInfo\")\r\n    @ApiOperation(\"获取邀请信息\")\r\n    @GetLoginUser\r\n    @Access\r\n    public Map<String, Object> inviteInfo(SysUserParam userParam) {\r\n        return mineService.getInviteInfo(userParam);\r\n    }\r\n\r\n    @PostMapping(\"/mine/myTaem\")\r\n    @ApiOperation(\"我的团队\")\r\n    @GetLoginUser\r\n    @Access\r\n    public Map<String, Object> myTaem(SysUserParam userParam) {\r\n        return mineService.myTeam(userParam);\r\n    }\r\n\r\n    @PostMapping(\"/mine/updatetradepassword\")\r\n    @ApiOperation(\"修改交易密码\")\r\n    @GetLoginUser\r\n    @Access\r\n    public void updateTradePassWord(SysUserParam sysUserParam, String phone, String code, String newTradePassWord, String newTradePassWord2) {\r\n        userService.updateTradePassWord(sysUserParam, phone, code, newTradePassWord, newTradePassWord2);\r\n    }\r\n\r\n    @PostMapping(\"/mine/level\")\r\n    @ApiOperation(\"等级详情\")\r\n    @GetLoginUser\r\n    @Access\r\n    public Map<String, Object> getLevel() {\r\n        Map<String, Object> map = new HashMap<>();\r\n        DecimalFormat decimalFormat = new DecimalFormat(\"##.00%\");\r\n        map.put(\"beanTradeCommission1\", decimalFormat.format(new BigDecimal(dictService.getValue(\"beanTradeCommission1\"))));\r\n        map.put(\"beanTradeCommission2\", decimalFormat.format(new BigDecimal(dictService.getValue(\"beanTradeCommission2\"))));\r\n        map.put(\"beanTradeCommission3\", decimalFormat.format(new BigDecimal(dictService.getValue(\"beanTradeCommission3\"))));\r\n        map.put(\"beanTradeCommission4\", decimalFormat.format(new BigDecimal(dictService.getValue(\"beanTradeCommission4\"))));\r\n        map.put(\"beanTradeCommission5\", decimalFormat.format(new BigDecimal(dictService.getValue(\"beanTradeCommission5\"))));\r\n        map.put(\"beanTradeCommission6\", decimalFormat.format(new BigDecimal(dictService.getValue(\"beanTradeCommission6\"))));\r\n        map.put(\"contributionLv2\", dictService.getValue(\"contributionLv2\"));\r\n        map.put(\"contributionLv3\", dictService.getValue(\"contributionLv3\"));\r\n        map.put(\"contributionLv4\", dictService.getValue(\"contributionLv4\"));\r\n        map.put(\"contributionLv5\", dictService.getValue(\"contributionLv5\"));\r\n        map.put(\"contributionLv6\", dictService.getValue(\"contributionLv6\"));\r\n        return map;\r\n\r\n    }\r\n\r\n    @PostMapping(\"/increasecontribution\")\r\n    @ApiOperation(\"增加贡献值PHP\")\r\n    public void increasecontribution(int userId) {\r\n        realNameService.zjgxz(userId);\r\n    }\r\n\r\n}\r\n
